{
  "manifest_version": 2,
  "name": "A New Homepage",
  "short_name": "a_new_home",
  "version": "0.1",
  "author": "Daniel PrÄ™tki",
  "description": "A new tab page Extansion",
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "browser_action": {
    "default_popup": "tool-bar-popup.html",
    "default_title": "add current page to bookmarks",
    "default_icon": {
      "16": "icons/white-extansion-icon.png",
      "32": "icons/white-extansion-icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "html": [
        "index.js"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}